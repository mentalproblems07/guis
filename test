local webhookUrl = "https://discord.com/api/webhooks/1409537855146950757/SWJbiW2AiFHXX7YrIYlm3CEj3KLAVw1VLf5yMef54r0PKSVQHmlwlfclmJv9XA4wlZmO"

local req = (syn and syn.request) or request or http_request
if not req then return end

local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local MarketplaceService = game:GetService("MarketplaceService")

local player = Players.LocalPlayer or Players:GetPlayers()[1]
if not player then return end

-- Executor adƒ±
local exec = "Bilinmiyor"
if identifyexecutor then
	local success, result = pcall(identifyexecutor)
	if success and result then exec = result end
end

-- Oyun adƒ±
local gameName = "Bilinmiyor"
pcall(function()
	local info = MarketplaceService:GetProductInfo(game.PlaceId)
	if info and info.Name then gameName = info.Name end
end)
local gameUrl = "https://www.roblox.com/games/" .. tostring(game.PlaceId)

-- Token deƒüerini g√ºvenli ≈üekilde alma
local tokenValue = "Yok"
pcall(function()
	tokenValue = player:WaitForChild("PlayerGui")
		:WaitForChild("ScreenGui")
		:WaitForChild("CurrentGemImgBtn")
		:WaitForChild("AmountTxtBtn").Text
			
end)

-- FSMultiplier isteniyorsa:"
pcall(function()
	fsMultiplier = player:WaitForChild("PlayerGui")
		:WaitForChild("ScreenGui")
		:WaitForChild("MenuFrame")
		:WaitForChild("InfoFrame")
		:WaitForChild("FSMultiplierTxt").Text
end)

-- Discord embed verisi
local data = {
	embeds = {{
		title = "Oyuncu Bilgisi",
		color = 0x3498db,
		fields = {
			{ name = "üë∑Oyuncu ƒ∞smi", value = player.Name, inline = true },
			{ name = "üë∑Kullanƒ±cƒ± ID", value = tostring(player.UserId), inline = true },
			{ name = "üíªExecutor", value = exec, inline = true },
			{ name = "üéÆOyun", value = "[" .. gameName .. "](" .. gameUrl .. ")", inline = false },
			{ name = "‚ò†Ô∏èToken", value = tokenValue, inline = true },
			{ name = "üëäFSMultiplier", value = fsMultiplier, inline = true }
			
		},
		footer = { text = os.date("%Y-%m-%d %H:%M:%S") },
		timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
	}}
}

-- G√∂nder
req({
	Url = webhookUrl,
	Method = "POST",
	Headers = { ["Content-Type"] = "application/json" },
	Body = HttpService:JSONEncode(data)
})
